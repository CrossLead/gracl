{
  "name": "gracl",
  "version": "0.0.9",
  "description": "graph acl in typescript",
  "main": "./dist/node4/lib/gracl.js",
  "typings": "./dist/es2015/lib/gracl.d.ts",
  "scripts": {
    "babel": "./node_modules/.bin/babel ./dist/es2015 --out-dir ./dist/node4",
    "build": "rm -rf ./dist/ && npm run typings && npm run lint && npm run tsc && npm run babel",
    "pretest": "npm run build && npm run lint",
    "test": "./node_modules/.bin/ava",
    "tsc": "./node_modules/typescript/bin/tsc",
    "typings": "./node_modules/typings/dist/bin/typings.js install",
    "docs": "bash -c \"rm -rf ./docs/ && ./node_modules/typedoc/bin/typedoc --mode file --theme default --target es6 --out ./docs/ ./lib/ && touch ./docs/.nojekyll\"",
    "gh-pages": "npm run docs && ./node_modules/gh-pages/bin/gh-pages --dotfiles true -d ./docs/",
    "lint": "./node_modules/tslint/bin/tslint -c ./tslint.json ./{test,lib}/**/*.ts",
    "prepublish": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CrossLead/gracl.git"
  },
  "author": "bsouthga@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CrossLead/gracl/issues"
  },
  "babel": {
    "presets": [
      "es2015-node4"
    ],
    "sourceMaps": true
  },
  "homepage": "https://github.com/CrossLead/gracl",
  "devDependencies": {
    "ava": "0.14.0",
    "babel-cli": "6.5.1",
    "babel-core": "6.5.2",
    "babel-polyfill": "6.5.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-es2015-node4": "2.0.3",
    "babel-register": "6.5.2",
    "babel-runtime": "6.5.0",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "chai": "3.5.0",
    "gh-pages": "0.10.0",
    "gulp": "3.9.1",
    "jasmine-core": "2.4.1",
    "lodash": "^3.10.1",
    "ts-node": "0.5.5",
    "tslint": "3.5.0",
    "typedoc": "sierrasoftworks/typedoc#v1.8.10",
    "typescript": "1.8.10",
    "typings": "0.6.8",
    "uglifyjs": "2.4.10"
  },
  "ava": {
    "files": [
      "dist/node4/test/spec/**/*.js"
    ]
  }
}
