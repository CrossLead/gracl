{
  "name": "gracl",
  "version": "0.0.0",
  "description": "graph acl in typescript",
  "main": "./dist/es5/gracl.js",
  "scripts": {
    "build": "npm run typings && npm run tscompile && npm run browserify && npm run minify",
    "pretest": "rm -rf ./dist/test && npm run tscompile",
    "test": "mocha dist/es6/test/spec/**/*.js --require babel-polyfill --compilers js:babel-core/register",
    "tscompile": "./node_modules/typescript/bin/tsc -p ./",
    "typings": "./node_modules/typings/dist/bin/typings.js install",
    "browserify": "mkdir -p ./dist/es5 && ./node_modules/browserify/bin/cmd.js ./dist/es6/lib/index.js -o ./dist/es5/gracl.js -t [ babelify --plugins [ transform-runtime ] ]",
    "minify": "./node_modules/uglifyjs/bin/uglifyjs ./dist/es5/gracl.js --screw-ie8 --mangle -o  ./dist/es5/gracl.min.js",
    "docs": "./node_modules/typedoc/bin/typedoc --theme minimal --experimentalAsyncFunctions --target es6 --out ./docs/ ./lib/",
    "ghpages": "npm run docs && node -e \"require('gulp').src('./docs/**/*').pipe(require('gulp-gh-pages')())\" && rm -rf ./.publish/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CrossLead/gracl.git"
  },
  "author": "bsouthga@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CrossLead/gracl/issues"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "sourceMaps": true
  },
  "homepage": "https://github.com/CrossLead/gracl",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.2",
    "babel-runtime": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-gh-pages": "^0.5.4",
    "jasmine-core": "^2.4.1",
    "mocha": "^2.4.5",
    "mocha-babel": "^3.0.3",
    "ts-node": "^0.5.5",
    "typedoc": "^0.3.12",
    "typescript": "^1.7.5",
    "typings": "^0.6.8",
    "uglifyjs": "^2.4.10"
  }
}
